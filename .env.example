# OnchainKit API Key (required for Base integration)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key_here

# OpenAI API Key (for generating rights guides and scripts)
OPENAI_API_KEY=your_openai_api_key_here

# Alternative: OpenRouter API Key (if using OpenRouter instead of OpenAI)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Supabase Configuration (for user data and incident storage)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Twilio Configuration (for SMS emergency alerts)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Pinata Configuration (for IPFS storage of rights cards)
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_API_KEY=your_pinata_secret_key_here

# Neynar API Key (for Farcaster integration)
NEYNAR_API_KEY=your_neynar_api_key_here
